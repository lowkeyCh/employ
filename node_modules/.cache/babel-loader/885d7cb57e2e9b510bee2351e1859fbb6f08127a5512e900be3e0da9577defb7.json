       // 移除组件\r\n                this.tableDataListShow = false\r\n                // 在组件移除后，重新渲染组件\r\n                // this.$nextTick可实现在DOM 状态更新后，执行传入的方法。\r\n                this.$nextTick(() => {\r\n                  this.tableDataListShow = true\r\n                })\r\n              })\r\n            })\r\n          })\r\n        })\r\n\r\n\r\n      }\r\n    })\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      /**\r\n       * 招聘过程集合\r\n       */\r\n      recruitmentProcessList: [{\r\n        rpId: 0,\r\n        graduateId: 0,\r\n        enId: 0,\r\n        rpPhase: '',\r\n      }],\r\n\r\n      /**\r\n       * 展示数据集合\r\n       */\r\n      tableDataList: [{\r\n        rpId: 0,\r\n        graduateId: 0,\r\n        enId: 0,\r\n        rpPhase: '',\r\n        graduateName: '',\r\n        quartersThird: '',\r\n        enterpriseName: '',\r\n      }],\r\n\r\n      /**\r\n       * 展示数据渲染\r\n       */\r\n      tableDataListShow: false\r\n    }\r\n  },\r\n\r\n  methods: {\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AA0DA;EACAA,QAAA;IACA,KAAAC,MAAA,CAAAC,GAAA,uCAAAC,IAAA,CAAAC,GAAA;MAGA,SAAAC,CAAA,MAAAA,CAAA,GAAAD,GAAA,CAAAE,IAAA,CAAAC,MAAA,EAAAF,CAAA;QACA,KAAAG,aAAA,CAAAH,CAAA;UACAI,IAAA,EAAAL,GAAA,CAAAE,IAAA,CAAAD,CAAA,EAAAI,IAAA;UACAC,UAAA,EAAAN,GAAA,CAAAE,IAAA,CAAAD,CAAA,EAAAK,UAAA;UACAC,IAAA,EAAAP,GAAA,CAAAE,IAAA,CAAAD,CAAA,EAAAM,IAAA;UACAC,OAAA,EAAAR,GAAA,CAAAE,IAAA,CAAAD,CAAA,EAAAO,OAAA;UACAC,YAAA;UACAC,aAAA;UACAC,cAAA;QACA;QAEA,KAAAC,sBAAA,CAAAX,CAAA;UACAI,IAAA,EAAAL,GAAA,CAAAE,IAAA,CAAAD,CAAA,EAAAI,IAAA;UACAC,UAAA,EAAAN,GAAA,CAAAE,IAAA,CAAAD,CAAA,EAAAK,UAAA;UACAC,IAAA,EAAAP,GAAA,CAAAE,IAAA,CAAAD,CAAA,EAAAM,IAAA;UACAC,OAAA,EAAAR,GAAA,CAAAE,IAAA,CAAAD,CAAA,EAAAO;QACA;QAEA,KAAAX,MAAA,CAAAC,GAAA,qEAAAE,GAAA,CAAAE,IAAA,CAAAD,CAAA,EAAAK,UAAA,EAAAP,IAAA,CAAAc,IAAA;UACA,KAAAT,aAAA,CAAAH,CAAA,EAAAQ,YAAA,GAAAI,IAAA,CAAAX,IAAA,CAAAO,YAAA;UAEA,KAAAZ,MAAA,CAAAC,GAAA,mDAAAE,GAAA,CAAAE,IAAA,CAAAD,CAAA,EAAAM,IAAA,EAAAR,IAAA,CAAAe,IAAA;YA