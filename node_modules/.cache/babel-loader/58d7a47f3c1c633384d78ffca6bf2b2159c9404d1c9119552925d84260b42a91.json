 = true;\r\n        }\r\n      })\r\n    },\r\n\r\n\r\n    methods: {\r\n      /**\r\n       * 文件上传自定义接口\r\n       * @param param\r\n       */\r\n      upload(param) {\r\n        console.log(param)\r\n        console.log(this.fileList)\r\n\r\n        this.fileList[this.fileList.length] = {name: \"1\", url: \"http://localhost:8081/resume/2b4d03bb-533a-418a-988f-d1d769fd00a0.png\"}\r\n      },\r\n\r\n      /**\r\n       * 移除文件\r\n       * @param file\r\n       */\r\n      handleRemove(file) {\r\n        console.log(file)\r\n        for(let i = 0; i != this.fileList.length; ++i) {\r\n          console.log(this.fileList[i])\r\n        }\r\n      },\r\n\r\n      /**\r\n       * 预览文件\r\n       * @param file\r\n       */\r\n      handlePictureCardPreview(file) {\r\n        this.preview.previewDialog = true;\r\n        this.preview.previewImageUrl = file.url;\r\n      },\r\n\r\n    },\r\n\r\n    data() {\r\n      return {\r\n        fileList: [{\r\n          name: \"1\",\r\n          url: \"http://localhost:8081/resume/2b4d03bb-533a-418a-988f-d1d769fd00a0.png\",\r\n        },{\r\n          name: \"2\",\r\n          url: \"http://localhost:8081/resume/2b4d03bb-533a-418a-988f-d1d769fd00a0.png\",\r\n        }],\r\n\r\n        /**\r\n         *  审核状态显示按钮\r\n         */\r\n        buttonShow: {\r\n          processShow: false,\r\n          successShow: false,\r\n          failShow: false,\r\n        },\r\n\r\n        /**\r\n         * 毕业生基本信息对象\r\n         */\r\n        graduate: {\r\n          graduateId: 0,\r\n          graduateName: '',\r\n          graduateAge: '',\r\n          graduateSex: '',\r\n          graduateBirthday: '',\r\n          graduateCollege: '',\r\n          graduateGraduateDate: '',\r\n          graduateMajor: '',\r\n          userId: 0,\r\n          graduatePhone: '',\r\n          graduateEmail: '',\r\n          graduateQualificationCheck: '',\r\n        },\r\n\r\n        /**\r\n         * 界面预览弹窗显示对象\r\n         */\r\n        preview: {\r\n          previewDialog: false,\r\n          previewImageUrl: '',\r\n        }\r\n      }\r\n    },\r\n  }\r\n</script>\r\n\r\n<template>\r\n\r\n\r\n  <el-card>\r\n    <el-descriptions class=\"margin-top\" title=\"毕业资格上传\" :column=\"2\" border>\r\n      <template slot=\"extra\">\r\n        <el-button v-show=\"buttonShow.processShow\" class=\"button\" type=\"warning\" size=\"medium\">\r\n          <i class=\"el-icon-loading\"></i>\r\n          毕业资格审查中\r\n        </el-button>\r\n\r\n        <el-button v-show=\"buttonShow.successShow\" class=\"button\" type=\"success\" size=\"medium\">\r\n          <i class=\"el-icon-check\"></i>\r\n          毕业资格审查成功\r\n        </el-button>\r\n\r\n        <el-button v-show=\"buttonShow.failShow\" class=\"button\" type=\"danger\" size=\"medium\">\r\n          <i class=\"el-icon-close\"></i>\r\n          毕业资格审查未通过\r\n        </el-button>\r\n      </template>\r\n    </el-descriptions>\r\n\r\n    <el-upload\r\n        action=\"\"\r\n        :http-request=\"upload\"\r\n        list-type=\"picture-card\"\r\n        :file-list=\"fileList\"\r\n        :auto-upload=\"true\"\r\n    >\r\n        <i slot=\"default\" class=\"el-icon-plus\"></i>\r\n\r\n        <div slot=\"file\" slot-scope=\"{fileList}\">\r\n          <img\r\n              class=\"el-upload-list__item-thumbnail\"\r\n              :src=\"fileList[0].url\" alt=\"\"\r\n          >\r\n          <span class=\"el-upload-list__item-actions\">\r\n\r\n            <span\r\n                class=\"el-upload-list__item-preview\"\r\n                @click=\"handlePictureCardPreview(file)\"\r\n            >\r\n              <i class=\"el-icon-zoom-in\"></i>\r\n            </span>\r\n\r\n            <span\r\n                class=\"el-upload-list__item-delete\"\r\n                @click=\"handleRemove(file)\"\r\n            >\r\n              <i class=\"el-icon-delete\"></i>\r\n            </span>\r\n\r\n          </span>\r\n        </div>\r\n    </el-upload>\r\n\r\n    <!--界面预览弹窗-->\r\n    <el-dialog :visible.sync=\"preview.previewDialog\">\r\n      <img width=\"100%\" :src=\"preview.previewImageUrl\" alt=\"\">\r\n    </el-dialog>\r\n  </el-card>\r\n\r\n</template>\r\n\r\n<style scoped>\r\n  .button {\r\n    color: black;\r\n    cursor: auto;\r\n  }\r\n\r\n  .button:hover {\r\n    border: 1px solid #000;\r\n  }\r\n\r\n</style>"],"mappings":"AACA;EACAA,QAAA;IACA;AACA;AACA;IACA,IAAAC,MAAA,GAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA,UAAAC,IAAA,CAAAL,MAAA;IAEA,KAAAM,MAAA,CAAAC,GAAA,kDAAAP,MAAA,EAAAQ,IAAA,CAAAC,GAAA;MACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;MACA,KAAAG,QAAA,CAAAC,UAAA,GAAAJ,GAAA,CAAAK,IAAA,CAAAD,UAAA;MACA,KAAAD,QAAA,CAAAG,YAAA,GAAAN,GAAA,CAAAK,IAAA,CAAAC,YAAA;MACA,KAAAH,QAAA,CAAAI,WAAA,GAAAP,GAAA,CAAAK,IAAA,CAAAE,WAAA;MACA,KAAAJ,QAAA,CAAAK,WAAA,GAAAR,GAAA,CAAAK,IAAA,CAAAG,WAAA;MACA,KAAAL,QAAA,CAAAM,gBAAA,GAAAT,GAAA,CAAAK,IAAA,CAAAI,gBAAA;MACA,KAAAN,QAAA,CAAAO,eAAA,GAAAV,GAAA,CAAAK,IAAA,CAAAK,eAAA;MACA,KAAAP,QAAA,CAAAQ,oBAAA,GAAAX,GAAA,CAAAK,IAAA,CAAAM,oBAAA;MACA,KAAAR,QAAA,CAAAS,aAAA,GAAAZ,GAAA,CAAAK,IAAA,CAAAO,aAAA;MACA,KAAAT,QAAA,CAAAZ,MAAA,GAAAS,GAAA,CAAAK,IAAA,CAAAd,MAAA;MACA,KAAAY,QAAA,CAAAU,aAAA,GAAAb,GAAA,CAAAK,IAAA,CAAAQ,aAAA;MACA,KAAAV,QAAA,CAAAW,aAAA,GAAAd,GAAA,CAAAK,IAAA,CAAAS,aAAA;MACA,KAAAX,QAAA,CAAAY,0BAAA,GAAAf,GAAA,CAAAK,IAAA,CAAAU,0BAAA;;MAEA;AACA;AACA;MACAd,OAAA,CAAAC,GAAA,MAAAC,QAAA,CAAAY,0BAAA;MACA,SAAAZ,QAAA,CAAAY,0BAAA;QACA,KAAAC,UAAA,CAAAC,WAAA;QACA,KAAAD,UAAA,CAAAE,WAAA;QACA,KAAAF,UAAA,CAAAG,QAAA;MACA,gBAAAhB,QAAA,CAAAY,0BAAA;QACA,KAAAC,UAAA,CAAAC,WAAA;QACA,KAAAD,UAAA,CAAAE,WAAA;QACA,KAAAF,UAAA,CAAAG,QAAA;MACA;QACA,KAAAH,UAAA,CAAAC,WAAA;QACA,KAAAD,UAAA,CAAAE,WAAA;QACA,KAAAF,UAAA,CAAAG,QAAA;MACA;IACA;EACA;EAGAC,OAAA;IACA;AACA;AACA;AACA;IACAC,OAAAC,KAAA;MACArB,OAAA,CAAAC,GAAA,CAAAoB,KAAA;MACArB,OAAA,CAAAC,GAAA,MAAAqB,QAAA;MAEA,KAAAA,QAAA,MAAAA,QAAA,CAAAC,MAAA;QAAAC,IAAA;QAAAC,GAAA;MAAA;IACA;IAEA;AACA;AACA;AACA;IACAC,aAAAC,IAAA;MACA3B,OAAA,CAAAC,GAAA,CAAA0B,IAAA;MACA,SAAAC,CAAA,MAAAA,CAAA,SAAAN,QAAA,CAAAC,MAAA,IAAAK,CAAA;QACA5B,OAAA,CAAAC,GAAA,MAAAq