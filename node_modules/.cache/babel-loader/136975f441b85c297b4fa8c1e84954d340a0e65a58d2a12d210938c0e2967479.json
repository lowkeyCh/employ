{"ast":null,"code":"export default {\n  mounted() {\n    /**\r\n     * 从数据库中读取所有职位\r\n     */\n    this.$axios.get('http://localhost:8081/quarters/query_all').then(res => {\n      for (let i = 0; i < res.data.length; i++) {\n        this.quartersList[i] = {\n          quartersId: res.data[i].quartersId,\n          quartersFirst: res.data[i].quartersFirst,\n          quartersSecond: res.data[i].quartersSecond,\n          quartersThird: res.data[i].quartersThird\n        };\n\n        /**\r\n         * 展示数据赋值\r\n         */\n        this.tableDataList[i] = {\n          quartersId: res.data[i].quartersId,\n          quartersFirst: res.data[i].quartersFirst,\n          quartersSecond: res.data[i].quartersSecond,\n          quartersThird: res.data[i].quartersThird\n        };\n      }\n\n      /**\r\n       * 重新渲染el-upload\r\n       */\n      // 移除组件\n      this.tableDataListShow = false;\n      // 在组件移除后，重新渲染组件\n      // this.$nextTick可实现在DOM 状态更新后，执行传入的方法。\n      this.$nextTick(() => {\n        this.tableDataListShow = true;\n      });\n    });\n  },\n  data() {\n    return {\n      /**\r\n       * 职位类型集合\r\n       */\n      quartersList: [{\n        quartersId: 0,\n        quartersFirst: '',\n        quartersSecond: '',\n        quartersThird: ''\n      }],\n      /**\r\n       * 展示数据集合\r\n       */\n      tableDataList: [{\n        quartersId: 0,\n        quartersFirst: '',\n        quartersSecond: '',\n        quartersThird: ''\n      }],\n      /**\r\n       * 展示数据渲染\r\n       */\n      tableDataListShow: false,\n      /**\r\n       * 添加对话框\r\n       */\n      addDialog: {\n        quarters: {\n          quartersFirst: '',\n          quartersSecond: '',\n          quartersThird: ''\n        },\n        addShowDialog: false\n      }\n    };\n  },\n  methods: {\n    addShow() {\n      this.addDialog.quarters = {\n        quartersFirst: '',\n        quartersSecond: '',\n        quartersThird: ''\n      };\n      this.addDialog.addShowDialog = true;\n    },\n    update() {},\n    delete() {}\n  }\n};","map":{"version":3,"names":["mounted","$axios","get","then","res","i","data","length","quartersList","quartersId","quartersFirst","quartersSecond","quartersThird","tableDataList","tableDataListShow","$nextTick","addDialog","quarters","addShowDialog","methods","addShow","update","delete"],"sources":["src/components/manager/ManagerQuarters.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"padding: 5px 20px 10px 0px\">\r\n      <el-input placeholder=\"领域/模块/职位\" style=\"width: 200px\" suffix-icon=\"el-icon-edit\"></el-input>\r\n      <el-button type=\"primary\" icon=\"el-icon-search\" style=\"margin-left: 10px\">查询</el-button>\r\n      <el-button @click=\"addShow\" type=\"success\" icon=\"el-icon-plus\" style=\"margin-left: 10px\">新建</el-button>\r\n    </div>\r\n\r\n    <el-table\r\n        :data=\"tableDataList\"\r\n        v-if=\"tableDataListShow\"\r\n        style=\"width: 100%\"\r\n    >\r\n      <el-table-column align=\"center\" label=\"ID\" width=\"100\" type=\"index\">\r\n      </el-table-column>\r\n\r\n      <el-table-column\r\n          prop=\"quartersFirst\"\r\n          label=\"领域\"\r\n          width=\"180\"\r\n      >\r\n      </el-table-column>\r\n\r\n      <el-table-column\r\n          prop=\"quartersSecond\"\r\n          label=\"模块\"\r\n          width=\"180\"\r\n      >\r\n      </el-table-column>\r\n\r\n      <el-table-column\r\n          prop=\"quartersThird\"\r\n          label=\"职位\"\r\n      >\r\n      </el-table-column>\r\n\r\n      <el-table-column\r\n          label=\"操作\"\r\n          width=\"100\"\r\n      >\r\n\r\n        <template slot-scope=\"scope\">\r\n          <el-button @click=\"update(scope.row)\" type=\"text\" size=\"small\">修改</el-button>\r\n          <el-button @click=\"delete(scope.row)\" type=\"text\" size=\"small\">删除</el-button>\r\n        </template>\r\n\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <el-dialog title=\"职业范畴\" :visible.sync=\"addDialog.addShowDialog\" style=\"margin-top: 20px; margin-bottom: 20px\">\r\n      <el-input placeholder=\"请输入领域\" v-model=\"addDialog.quarters.quartersFirst\" style=\"height: 30px\">\r\n        <template slot=\"prepend\">领域</template>\r\n      </el-input>\r\n      <el-input placeholder=\"请输入模块\" v-model=\"addDialog.quarters.quartersSecond\">\r\n        <template slot=\"prepend\">模块</template>\r\n      </el-input>\r\n      <el-input placeholder=\"请输入职位\" v-model=\"addDialog.quarters.quartersThird\">\r\n        <template slot=\"prepend\">职位</template>\r\n      </el-input>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  mounted() {\r\n    /**\r\n     * 从数据库中读取所有职位\r\n     */\r\n    this.$axios.get('http://localhost:8081/quarters/query_all').then(res=>{\r\n      for(let i = 0; i < res.data.length; i++) {\r\n        this.quartersList[i] = {\r\n          quartersId: res.data[i].quartersId,\r\n          quartersFirst: res.data[i].quartersFirst,\r\n          quartersSecond: res.data[i].quartersSecond,\r\n          quartersThird: res.data[i].quartersThird,\r\n        }\r\n\r\n        /**\r\n         * 展示数据赋值\r\n         */\r\n        this.tableDataList[i] = {\r\n          quartersId: res.data[i].quartersId,\r\n          quartersFirst: res.data[i].quartersFirst,\r\n          quartersSecond: res.data[i].quartersSecond,\r\n          quartersThird: res.data[i].quartersThird,\r\n        }\r\n      }\r\n\r\n      /**\r\n       * 重新渲染el-upload\r\n       */\r\n      // 移除组件\r\n      this.tableDataListShow = false\r\n      // 在组件移除后，重新渲染组件\r\n      // this.$nextTick可实现在DOM 状态更新后，执行传入的方法。\r\n      this.$nextTick(() => {\r\n        this.tableDataListShow = true\r\n      })\r\n    })\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      /**\r\n       * 职位类型集合\r\n       */\r\n      quartersList:[{\r\n        quartersId: 0,\r\n        quartersFirst: '',\r\n        quartersSecond: '',\r\n        quartersThird: '',\r\n      }],\r\n\r\n      /**\r\n       * 展示数据集合\r\n       